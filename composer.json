{
  "name": "khan-zia/laravel-saml-idp",
  "description": "Make your Laravel application an Idenification Provider using SAML 2.0.",
  "keywords": [
    "laravel",
    "saml",
    "saml 2.0",
    "auth",
    "acl",
    "sso",
    "idp"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "illuminate/support": "^7.0",
    "illuminate/routing": "^7.0",
    "forlabs/lightsaml": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ziakhan\\SamlIdp\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ziakhan\\SamlIdp\\LaravelSamlIdpServiceProvider"
      ]
    }
  }
}
